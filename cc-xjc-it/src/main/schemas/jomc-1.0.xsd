  <xsd:element name="authors" type="jomc:Authors"/>
  <xsd:complexType name="Authors">
    <xsd:annotation>
      <xsd:appinfo>
        <jaxb:class>
           <jaxb:javadoc><![CDATA[List of authors.]]></jaxb:javadoc>
        </jaxb:class>
      </xsd:appinfo>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="jomc:ModelObject">
        <xsd:choice>
          <xsd:element name="author" type="jomc:Author" minOccurs="0"
                       maxOccurs="unbounded"/>
        </xsd:choice>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <!--                                                                       -->
  <!-- Author                                                                -->
  <!--                                                                       -->
  <xsd:element name="author" type="jomc:Author"/>
  <xsd:complexType name="Author">
    <xsd:annotation>
      <xsd:appinfo>
        <jaxb:class>
          <jaxb:javadoc><![CDATA[Author.]]></jaxb:javadoc>
        </jaxb:class>
      </xsd:appinfo>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="jomc:ModelObject">
        <xsd:attribute name="name" type="jomc:String" use="required">
          <xsd:annotation>
            <xsd:appinfo>
              <jaxb:property>
                <jaxb:javadoc><![CDATA[Name of the author.]]></jaxb:javadoc>
              </jaxb:property>
            </xsd:appinfo>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="version" type="jomc:Version" use="optional">
          <xsd:annotation>
            <xsd:appinfo>
              <jaxb:property>
                <jaxb:javadoc><![CDATA[The version of the object created due to the author.]]></jaxb:javadoc>
              </jaxb:property>
            </xsd:appinfo>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="mail" type="jomc:String" use="optional">
          <xsd:annotation>
            <xsd:appinfo>
              <jaxb:property>
                <jaxb:javadoc><![CDATA[Mail address of the author.]]></jaxb:javadoc>
              </jaxb:property>
            </xsd:appinfo>
          </xsd:annotation>
        </xsd:attribute>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <!--                                                                       -->
  <!-- Simple types.                                                         -->
  <!--                                                                       -->
  <xsd:simpleType name="String">
    <xsd:restriction base="xsd:string"/>
  </xsd:simpleType>
  <xsd:simpleType name="Version">
    <xsd:annotation>
      <xsd:documentation xml:lang="en"><![CDATA[
        Restricts "xsd:string" to a maximum length of 100 characters.]]>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:maxLength value="100"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="Identifier">
    <xsd:annotation>
      <xsd:documentation xml:lang="en"><![CDATA[
        Identifier of an entity,]]>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:pattern value="\p{L}.*"/>
      <xsd:whiteSpace value="collapse"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="PropertyType">
    <xsd:annotation>
      <xsd:appinfo>
        <jaxb:typesafeEnumClass>
          <jaxb:javadoc><![CDATA[Enumeration of property types.]]></jaxb:javadoc>
        </jaxb:typesafeEnumClass>
      </xsd:appinfo>
    </xsd:annotation>
    <xsd:restriction base="xsd:NCName">
      <xsd:enumeration value="byte"/>
      <xsd:enumeration value="short"/>
      <xsd:enumeration value="int"/>
      <xsd:enumeration value="long"/>
      <xsd:enumeration value="char"/>
      <xsd:enumeration value="float"/>
      <xsd:enumeration value="double"/>
      <xsd:enumeration value="boolean"/>
      <xsd:enumeration value="java.lang.Byte"/>
      <xsd:enumeration value="java.lang.Short"/>
      <xsd:enumeration value="java.lang.Integer"/>
      <xsd:enumeration value="java.lang.Long"/>
      <xsd:enumeration value="java.lang.Character"/>
      <xsd:enumeration value="java.lang.Float"/>
      <xsd:enumeration value="java.lang.Double"/>
      <xsd:enumeration value="java.lang.Boolean"/>
      <xsd:enumeration value="java.lang.String"/>
      <xsd:enumeration value="java.lang.Object"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="ArgumentType">
    <xsd:annotation>
      <xsd:appinfo>
        <jaxb:typesafeEnumClass>
          <jaxb:javadoc><![CDATA[Enumeration of argument types.]]></jaxb:javadoc>
        </jaxb:typesafeEnumClass>
      </xsd:appinfo>
    </xsd:annotation>
    <xsd:restriction base="xsd:NCName">
      <xsd:enumeration value="Number"/>
      <xsd:enumeration value="Date"/>
      <xsd:enumeration value="Time"/>
      <xsd:enumeration value="Text"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="Multiplicity">
    <xsd:annotation>
      <xsd:appinfo>
        <jaxb:typesafeEnumClass>
          <jaxb:javadoc><![CDATA[Enumeration of multiplicities.]]></jaxb:javadoc>
        </jaxb:typesafeEnumClass>
      </xsd:appinfo>
    </xsd:annotation>
    <xsd:restriction base="xsd:NCName">
      <xsd:enumeration value="One">
        <xsd:annotation>
          <xsd:appinfo>
            <jaxb:typesafeEnumMember name="ONE">
              <jaxb:javadoc><![CDATA[
A multiplicity equal to {@code ONE} specifies that no more than one
implementation of the specification is allowed to exist among a set of modules
(including none).]]></jaxb:javadoc>
            </jaxb:typesafeEnumMember>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Many">
        <xsd:annotation>
          <xsd:appinfo>
            <jaxb:typesafeEnumMember name="MANY">
              <jaxb:javadoc><![CDATA[
A multiplicity equal to {@code MANY} specifies that many implementations of the
specification are allowed to exist among a set of modules (including none).]]></jaxb:javadoc>
            </jaxb:typesafeEnumMember>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="Scope">
    <xsd:annotation>
      <xsd:appinfo>
        <jaxb:typesafeEnumClass>
          <jaxb:javadoc><![CDATA[Enumeration of scopes.]]></jaxb:javadoc>
        </jaxb:typesafeEnumClass>
      </xsd:appinfo>
    </xsd:annotation>
    <xsd:restriction base="xsd:NCName">
      <xsd:enumeration value="Multiton">
        <xsd:annotation>
          <xsd:appinfo>
            <jaxb:typesafeEnumMember name="MULTITON">
              <jaxb:javadoc><![CDATA[
In multiton scope, a new instance is created whenever requested.]]></jaxb:javadoc>
            </jaxb:typesafeEnumMember>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Context">
        <xsd:annotation>
          <xsd:appinfo>
            <jaxb:typesafeEnumMember name="CONTEXT">
              <jaxb:javadoc><![CDATA[
In context scope, instances are bound to a system's context. An instance is only
created if not already available in context.]]></jaxb:javadoc>
            </jaxb:typesafeEnumMember>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="Singleton">
        <xsd:annotation>
          <xsd:appinfo>
            <jaxb:typesafeEnumMember name="SINGLETON">
              <jaxb:javadoc><![CDATA[
In singleton scope, instances are bound to a system's single instance store. An
instance is only created if not already available in that single instance store.]]></jaxb:javadoc>
            </jaxb:typesafeEnumMember>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:enumeration>
    </xsd:restriction>
  </xsd:simpleType>
</xsd:schema>
